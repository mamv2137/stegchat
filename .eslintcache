Context = createContext({});\r\n\r\nconst ChatProvider = ({ children }) => {\r\n  const [state, dispatch] = useReducer(chatReducer, INIT_STATE)\r\n  const [userState] = useContextUser()\r\n\r\n  const getChatsByUser = () => {\r\n    const { user } = userState\r\n    if (!user) return []\r\n    const chatsIds = user.chats\r\n    const chatList = chatsIds.map(id => {\r\n      const chat = chats.find(chat => chat.id === id)\r\n      return Object.assign({}, chat, {\r\n        isSelected: false\r\n      })\r\n    }).map(chat => {\r\n      const contact = users.find(user => user.userId === chat.contact)\r\n      return Object.assign({}, chat, {\r\n        contact\r\n      })\r\n    })\r\n\r\n    return chatList\r\n  }\r\n \r\n  useEffect(() => {\r\n    const userChats = getChatsByUser();\r\n    dispatch(setChats(userChats))\r\n  }, [userState])\r\n  \r\n  return (\r\n    <ChatContext.Provider\r\n      value={{\r\n        chatState: state,\r\n        chatDispatch: dispatch,\r\n      }}\r\n    >\r\n      {children}\r\n    </ChatContext.Provider>\r\n  );\r\n};\r\n\r\nconst useContextChat = () => {\r\n  const { chatState, chatDispatch } = useContext(ChatContext)\r\n\r\n  return [ chatState, chatDispatch ]\r\n}\r\n\r\nexport { ChatContext, ChatProvider, useContextChat };\r\n","/home/mamv2137/proyects/stegchat/src/components/Chat/Chat.styles.tw.js",[],"/home/mamv2137/proyects/stegchat/src/contexts/Chat/actions.js",[],"/home/mamv2137/proyects/stegchat/src/contexts/User/actions.js",[],"/home/mamv2137/proyects/stegchat/src/contexts/User/reducer.js",[],"/home/mamv2137/proyects/stegchat/src/contexts/Chat/reducer.js",[],"/home/mamv2137/proyects/stegchat/src/mockData/users.js",["198"],"export default [\r\n  {\r\n    \"userId\": \"f12e3e96-cb27-48b6-926b-7b5a1604901c\",\r\n    \"email\": \"mamv.2137@gmail.com\",\r\n    \"name\": \"mauricio alejandro\",\r\n    \"subname\": \"Life goes on...🍃\",\r\n    \"status\": \"Online\", //[\"Online\", \"Offline\", \"Away\"]\r\n    \"lastConnection