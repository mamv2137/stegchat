ser();\r\n    dispatch(setChats(userChats))\r\n  }, [userState])\r\n  \r\n  return (\r\n    <ChatContext.Provider\r\n      value={{\r\n        chatState: state,\r\n        chatDispatch: dispatch,\r\n      }}\r\n    >\r\n      {children}\r\n    </ChatContext.Provider>\r\n  );\r\n};\r\n\r\nconst useContextChat = () => {\r\n  const { chatState, chatDispatch } = useContext(ChatContext)\r\n\r\n  return [ chatState, chatDispatch ]\r\n}\r\n\r\nexport { ChatContext, ChatProvider, useContextChat };\r\n","/home/mamv2137/proyects/stegchat/src/components/Chat/Chat.styles.tw.js",[],"/home/mamv2137/proyects/stegchat/src/contexts/Chat/actions.js",[],"/home/mamv2137/proyects/stegchat/src/contexts/User/actions.js",[],"/home/mamv2137/proyects/stegchat/src/contexts/User/reducer.js",[],"/home/mamv2137/proyects/stegchat/src/contexts/Chat/reducer.js",[],"/home/mamv2137/proyects/stegchat/src/mockData/users.js",["220"],"export default [\r\n  {\r\n    userId: 'f12e3e96-cb27-48b6-926b-7b5a1604901c',\r\n    email: 'mamv.2137@gmail.com',\r\n    name: 'mauricio alejandro',\r\n    subname: 'Life goes on...ðŸƒ',\r\n    status: 'Online', //[\"Online\", \"Offline\", \"Away\"]\r\n    lastConnection: '',\r\n    chats: [\r\n      '50aaae27-06ee-41d5-bc9a-ad06e14f8d27',\r\n      '32aa4b4a-18f1-4e3d-82a2-7b5d6d73af2b',\r\n    ],\r\n    contacts: [\r\n      '564a4d6c-da8c-4718-bce9-0c89dfee7c45',\r\n      '2e675eca-a62d-4d53-a09a-6542ff3e175b',\r\n    ],\r\n    avatar:\r\n      'https://avataaars.io/?avatarStyle=Circle&topType=ShortHairTheCaesarSidePart&accessoriesType=Prescription01&hairColor=BrownDark&facialHairType=BeardLight&facialHairColor=Blonde&clotheType=BlazerSweater&eyeType=EyeRoll&eyebrowType=UnibrowNatural&mouthType=Sad&skinColor=Pale',\r\n  },\r\n  {\r\n    userId: '564a4d6c-da8c-4718-bce9-0c89dfee7c45',\r\n    email: 'usuario2@gmail.com',\r\n    name: 'Usuario 2',\r\n    subname: 'Faminazi forever...ðŸ¤º',\r\n    status: 'Online', //[\"Online\", \"Offline\", \"Away\"]\r\n    lastConnection: